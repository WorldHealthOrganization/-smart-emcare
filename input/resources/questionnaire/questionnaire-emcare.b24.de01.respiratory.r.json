                        "expression": "iif(%resource.repeat(item).where(linkId='Fast Breathing profile').answer.first().empty() and (%resource.repeat(item).where(linkId='AgeInMonths').answer.first().value >= 2 or %resource.repeat(item).where(linkId='second').repeat(item).where(linkId='EmCare.B24.G.DE03').answer.first().exists() or %resource.repeat(item).where(linkId='EmCare.B24.G.DE01').answer.first().value < 60 '{Breaths}/min' or ( %resource.repeat(item).where(linkId='Respiratory Rate (Second Count) Not Possible').answer.first().exists() and  ( %resource.repeat(item).where(linkId='EmCare.B24.G.DE01').answer.first().value  >= 60 '{Breaths}/min'  ) and %resource.repeat(item).where(linkId='force-collection').answer.first().value=true )), iif( (%resource.repeat(item).where(linkId='AgeInMonths').answer.first().value<2 and (%resource.repeat(item).where(linkId='second').repeat(item).where(linkId='EmCare.B24.G.DE03').answer.first().value >= 60 '{Breaths}/min' or (%resource.repeat(item).where(linkId='Respiratory Rate (Second Count) Not Possible').answer.first().exists()  and ( %resource.repeat(item).where(linkId='EmCare.B24.G.DE01').answer.first().value  >= 60 '{Breaths}/min'  or %resource.repeat(item).where(linkId='EmCare.B24.G.DE01').answer.first().value  >= 60 '{Breaths}/min' ) ) ) ) or  (%resource.repeat(item).where(linkId='EmCare.B24.G.DE01').answer.first().value >= 50 '{Breaths}/min' and %resource.repeat(item).where(linkId='AgeInMonths').answer.first().value < 12 and %resource.repeat(item).where(linkId='AgeInMonths').answer.first().value >= 2) or  (%resource.repeat(item).where(linkId='EmCare.B24.G.DE01').answer.first().value >= 40 '{Breaths}/min' and %resource.repeat(item).where(linkId='AgeInMonths').answer.first().value>=12 and %resource.repeat(item).where(linkId='AgeInMonths').answer.first().value < 60) , true, false),{})"
